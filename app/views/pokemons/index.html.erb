<div class="jumbotron">
  <h1 class="mx-auto">Pokemon List</h1>
</div>
<table class="pt-5 mx-auto">
	<% @pokemons.each do |pokemon| %>
    <tr>
      <td><%= pokemon.name %></td>
      <td><%= pokemon.id %></td>
      <td><%= link_to 'Edit', edit_pokemon_path(id: pokemon.id) %></td>
      <td><%= link_to 'Show', pokemon_path(id: pokemon.id) %></td>
    </tr>
	<% end %>
</table>
